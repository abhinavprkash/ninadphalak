package stronglyConnectedComponents;


public class Node {
	int nodeId;
	boolean explored = false;
	
	public Node(int nodeId){
		this.nodeId = nodeId;
	}
	public Node(){
		
	}
	
	public void markUnexplored(){
		this.explored=false;
	}
	

	@Override
	public String toString() {

		return nodeId.toString();
	}
}
